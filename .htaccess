<IfModule mod_rewrite.c>
  RewriteEngine on
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

RewriteCond %{HTTPS} off
RewriteCond %{HTTP:X-Forwarded-SSL} !on
RewriteCond %{HTTP_HOST} ^planetandpower\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.planetandpower\.com$
RewriteRule ^/?$ "https\:\/\/planetandpower\.com\/" [R=301,L]